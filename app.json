{
  "pages": [
    "pages/weather/weather",
    "pages/map/map",
    "pages/chat/chat"
  ],
  "window": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "呆鸣",
    "navigationBarBackgroundColor": "#FFFFFF",
    "navigationStyle": "default"
  },
  "tabBar": {
    "list": [
    {
      "pagePath": "pages/weather/weather",
      "text": "天气",
      "iconPath": "./static/icon/cloud.png",
      "selectedIconPath": "./static/icon/cloud-active.png"
    },
    {
      "pagePath": "pages/map/map",
      "text": "地图",
      "iconPath": "./static/icon/map.png",
      "selectedIconPath": "./static/icon/map-active.png"
    },
    {
      "pagePath": "pages/chat/chat",
      "text": "聊天",
      "iconPath": "./static/icon/bot.png",
      "selectedIconPath": "./static/icon/bot-active.png"
    }
  ]},
  "requiredPrivateInfos": [
    "getLocation"
  ],
  "permission": {
    "scope.userLocation": {
      "desc": "你的位置信息将用于小程序位置接口的效果展示"
    }
  },
  "style": "v2",
  "rendererOptions": {
    "skyline": {
      "defaultDisplayBlock": true,
      "disableABTest": true,
      "sdkVersionBegin": "3.0.0",
      "sdkVersionEnd": "15.255.255"
    }
  },
  "componentFramework": "glass-easel",
  "sitemapLocation": "sitemap.json",
  "lazyCodeLoading": "requiredComponents"
}